function visualize_stopping(data);
    
    cities16 = data(1,:,:);
    cities18 = data(2,:,:);
    cities23 = data(3,:,:);
    cities25 = data(4,:,:);
    cities48 = data(5,:,:);
    cities50 = data(6,:,:);
    cities51 = data(7,:,:);
    cities67 = data(8,:,:);
    cities70 = data(9,:,:);
    cities100 = data(10,:,:);
    cities127 = data(11,:,:);
    
    j = 1;
    x = [1:6];
    for i=1:7;
        hold on;
        labels = ["5","10", "20", "25", "30", "35", "none"];
        ylabel("optimal tour length");
        xlabel("parameter settings");
        xticks([1:7]);
        xticklabels(["0.05","0.01", "0.005", "0.001", "0.0005", "none"]);
        scatter(x,cities16(:,j:j+5,1), 'filled');
        %scatter(x,cities18(:,:,1), 'filled');
        %scatter(x,cities23(:,:,1), 'filled');
        %scatter(x,cities25(:,:,1), 'filled');
        
        j = i+5;
        
    end
    legend(labels);
    sz = linspace(1,100,200);
    hold off;
    

end

